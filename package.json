{
  "name": "converter",
  "author": "icecreamparlor",
  "license": "MIT",
  "repository": "https://github.com/icecreamparlor/converter",
  "displayName": "Converter",
  "description": "Converter for development",
  "version": "1.0.2",
  "publisher": "icecreamparlor",
  "icon": "assets/mipong.jpeg",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "converter.convert",
        "title": "Converter - Convert Text"
      }
    ],
    "keybindings": [
      {
        "command": "converter.convert",
        "key": "cmd+shift+c",
        "when": "editorFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "vscode:publish": "vsce publish",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/node-forge": "^1.3.2",
    "@types/prettier": "^2.7.2",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2"
  },
  "dependencies": {
    "generate-schema": "^2.6.0",
    "iconv-lite": "^0.6.3",
    "json-to-ts": "^1.7.0",
    "node-forge": "^1.3.1",
    "prettier": "^2.8.8",
    "xml-js": "^1.6.11",
    "yaml": "^2.3.1"
  }
}
